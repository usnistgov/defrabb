{  
  "default_s3_bucket": "giab-data",
  "default_s3_path": "defrabb_runs",
  "release_rules": {  
    "local": {  
      "include": [  
        "results/",
        "results/**",  
        "resources/",
        "resources/comparison/",
        "resources/comparison/**",  
        "resources/exclusions/",  
        "resources/exclusions/**",  
        "config/resources.yml",  
        "config/analyses_{RUNID}.tsv",  
        "{RUNID}.tar.gz",  
        "environment.yml",  
        "run.log",  
        "run_README.md"  
      ],  
      "exclude": [  
        "*"
      ]  
    },  
    "s3": "same as local"  
  }  
}